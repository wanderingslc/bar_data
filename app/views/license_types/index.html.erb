<p id="notice"><%= notice %></p>

<h1>License Types</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Description</th>
      <th>Url</th>
      <th>Hours</th>
      <th>Alcohol types</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @license_types.each do |license_type| %>
      <tr>
        <td><%= license_type.code %></td>
        <td><%= license_type.description %></td>
        <td><%= license_type.url %></td>
        <td><%= license_type.hours %></td>
        <td><%= license_type.alcohol_types %></td>
        <td><%= link_to 'Show', license_type %></td>
        <td><%= link_to 'Edit', edit_license_type_path(license_type) %></td>
        <td><%= link_to 'Destroy', license_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New License Type', new_license_type_path %>
